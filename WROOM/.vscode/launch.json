    "version": "0.2.0",
    "configurations": [
        {
            "name": "OpenOCD Debug",
            "type": "gdb",
            "request": "attach",
            "executable": "./build/hello-world.elf",
            "target": ":3333",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "C:/msys32/opt/xtensa-esp32-elf/bin/xtensa-esp32-elf-gdb.exe",
            "autorun": [
                //"interrupt",
                "mon reset halt",
                "thb app_main"
            ],
            "printCalls": true,
            //"preLaunchTask": "openOCD"
        }
    ]
}